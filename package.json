{
  "name": "user-log",
  "version": "1.0.0",
  "description": "The user log",
  "main": "dist/app.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc",
    "start": "NODE_OPTIONS='--import tsx' node ./src/app.ts",
    "dev": "NODE_OPTIONS='--import tsx' tsx watch src/app.ts",
    "test": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest",
    "test:integration": "dotenv -e .env.test jest --config=jest.config.cjs --detectOpenHandles",
    "test:docker": "docker-compose -f docker-compose.yml down -v && docker-compose -f docker-compose.yml up test-runner --exit-code-from test-runner",
    "test:e2e": "NODE_ENV=test jest --config=jest.config.cjs --testMatch=\"**/test/e2e/**/*.test.ts\""
  },
  "dependencies": {
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "ioredis": "^5.6.1",
    "knex": "^3.1.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "tsx": "^4.7.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@jest/globals": "^30.0.5",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^9.0.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.3",
    "ts-jest": "^29.1.2",
    "typescript": "^5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Leon3854/user-log.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Leon3854/user-log/issues"
  },
  "homepage": "https://github.com/Leon3854/user-log#readme"
}
